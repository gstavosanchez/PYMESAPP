@model List<F2_201801351.Models.EstanteModel>
@{
    ViewBag.Title = "ViewEstante";
}
<fieldset>
    <legend>
        Estante
    </legend>
    <div class="row">
        @foreach (var m in Model)
        {
            <div class="col-sm-6 col-md-4">
                <div class="thumbnail">
                    <img src="https://image.flaticon.com/icons/svg/32/32152.svg" alt="..." height="200" width="200">
                    <div class="caption">
                        <h3>Codigo: @m.codigo </h3>
                        <p>Medidas:@m.ancho X @m.largo m</p>
                        <p>Codigo Pasillo:@m.idPasillo</p>
                        <p><a class="btn btn-danger" href="~/Home/New/@m.idEstante">Eliminar</a> </p>
                    </div>

                </div>

            </div>

        }
    </div>
</fieldset>

