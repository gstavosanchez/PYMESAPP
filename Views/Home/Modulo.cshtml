@model List<F2_201801351.Models.ModuloModel> 
@{
    ViewBag.Title = "Modulo";
}

    <fieldset>
        <legend>
            Mis Modulos
        </legend>
        <div class="row">
            @foreach (var m in Model)
            {
                <div class="col-sm-6 col-md-4">
                    <div class="thumbnail">
                        <img src="https://i.ya-webdesign.com/images/como-editar-imagenes-png.png" alt="..." height="200" width="200">
                        <div class="caption">
                            <h3>@m.abreviatura</h3>
                            <p>@m.nombre</p>
                            <p><a class="btn btn-danger" href="~/Home/New/@m.id">Eliminar</a> </p>
                        </div>

                    </div>

                </div>

            }


        </div>
    </fieldset>

